<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserEditFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageView14"
            android:layout_width="29dp"
            android:layout_height="29dp"
            android:src="@drawable/icon_user"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.089"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/edtTextUPName"
            app:layout_constraintBottom_toTopOf="@+id/edtTextUPUName"
            app:layout_constraintVertical_bias="0.384" />

        <EditText
            android:id="@+id/edtTextUPUName"
            android:layout_width="354dp"
            android:layout_height="35dp"
            android:background="@drawable/custom_txtbox"
            android:elevation="10dp"
            android:ems="10"
            android:inputType="text"
            android:paddingLeft="10dp"
            android:textColor="@color/gray"
            app:layout_constraintBottom_toTopOf="@+id/imageView8"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView14"
            app:layout_constraintVertical_bias="0.435"
            tools:ignore="TouchTargetSizeCheck" />

        <TextView
            android:id="@+id/txtCopyrights3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/ruda_bold"
            android:text="Â© Copyright All Rights Reserved"
            android:textAlignment="viewStart"
            android:textColor="@color/gray"
            android:textSize="10sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.977"
            tools:ignore="SmallSp,TextSizeCheck" />

        <Button
            android:id="@+id/btnUserProfileCancel"
            android:layout_width="150dp"
            android:layout_height="50dp"
            android:background="@drawable/custom_button1"
            android:fontFamily="@font/ruda_bold"
            android:text="CANCEL"
            android:textColor="@color/dark_blue"
            android:textSize="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.873"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.929" />

        <EditText
            android:id="@+id/edtTextUPName"
            android:layout_width="354dp"
            android:layout_height="35dp"
            android:background="@drawable/custom_txtbox"
            android:elevation="10dp"
            android:ems="10"
            android:inputType="text"
            android:paddingLeft="10dp"
            android:textColor="@color/gray"
            app:layout_constraintBottom_toTopOf="@+id/imageView14"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView12"
            app:layout_constraintVertical_bias="0.31"
            tools:ignore="TouchTargetSizeCheck" />

        <EditText
            android:id="@+id/edtTxtUPPassword"
            android:layout_width="354dp"
            android:layout_height="35dp"
            android:background="@drawable/custom_txtbox"
            android:elevation="10dp"
            android:ems="10"
            android:inputType="text"
            android:paddingLeft="10dp"
            android:password="true"
            android:textColor="@color/gray"
            app:layout_constraintTop_toBottomOf="@+id/imageView8"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/imageView9"
            app:layout_constraintVertical_bias="0.68"
            tools:ignore="TouchTargetSizeCheck" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/ruda_bold"
            android:text="Password"
            android:textColor="@color/black"
            android:textSize="18sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.221"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/edtTxtUPPassword"
            app:layout_constraintTop_toBottomOf="@+id/edtTextUPUName"/>

        <CheckBox
            android:id="@+id/chckBoxUPShowPass"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:buttonTint="@color/dark_blue"
            android:checked="false"
            android:text="Show Password"
            android:textColor="@color/gray"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/edtTxtUPPassword"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.9"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/edtTxtUPGender"
            tools:ignore="TouchTargetSizeCheck" />

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="29dp"
            android:layout_height="29dp"
            android:src="@drawable/icon_lock"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.089"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/edtTxtUPPassword"
            app:layout_constraintTop_toBottomOf="@+id/edtTextUPUName"
            app:layout_constraintVertical_bias="0.62" />

        <EditText
            android:id="@+id/edtTxtUPGender"
            android:layout_width="354dp"
            android:layout_height="35dp"
            android:background="@drawable/custom_txtbox1"
            android:clickable="false"
            android:editable="false"
            android:elevation="10dp"
            android:ems="10"
            android:focusable="false"
            android:inputType="text"
            android:paddingLeft="10dp"
            android:textColor="@color/gray"
            app:layout_constraintBottom_toTopOf="@+id/btnUserProfileSave"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.491"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView9"
            app:layout_constraintVertical_bias="0.804"
            tools:ignore="TouchTargetSizeCheck" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/ruda_bold"
            android:text="Name"
            android:textColor="@color/black"
            android:textSize="18sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.202"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imgViewUPProfile"
            app:layout_constraintBottom_toTopOf="@+id/edtTextUPName"/>

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/ruda_bold"
            android:text="Username"
            android:textColor="@color/black"
            android:textSize="18sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.226"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/edtTextUPName"
            app:layout_constraintBottom_toTopOf="@+id/edtTextUPUName"/>

        <TextView
            android:id="@+id/textView18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/ruda_bold"
            android:text="Gender"
            android:textColor="@color/black"
            android:textSize="18sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.209"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/edtTxtUPGender"
            app:layout_constraintTop_toBottomOf="@+id/edtTxtUPPassword"/>

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/imgViewUPProfile"
            android:layout_width="110dp"
            android:layout_height="110dp"
            android:src="@drawable/icon_boy"
            app:civ_border_color="#000000"
            app:civ_border_overlay="false"
            app:civ_border_width="1dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.111" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/ruda_black"
            android:text="User Profile"
            android:textColor="@color/black"
            android:textSize="38sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.126"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="29dp"
            android:layout_height="29dp"
            android:src="@drawable/icon_gender"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.089"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/edtTxtUPGender"
            app:layout_constraintTop_toBottomOf="@+id/edtTxtUPPassword"
            app:layout_constraintVertical_bias="0.74" />

        <Button
            android:id="@+id/btnUserProfileSave"
            android:layout_width="150dp"
            android:layout_height="50dp"
            android:background="@drawable/custom_button"
            android:fontFamily="@font/ruda_bold"
            android:text="SAVE"
            android:textColor="@color/white"
            android:textSize="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.107"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.929" />

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="29dp"
            android:layout_height="29dp"
            android:src="@drawable/icon_user"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.089"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imgViewUPProfile"
            app:layout_constraintBottom_toTopOf="@+id/edtTextUPName"
            app:layout_constraintVertical_bias="0.26" />

        <ImageView
            android:id="@+id/imgViewUPExit"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:src="@drawable/custom_exit"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.956"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>